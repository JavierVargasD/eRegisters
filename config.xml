<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ingecsa.eregisters.mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>E-registers</name>
    <description>
        Aplicacion para la gestion de actas ambientales
    </description>
    <author email="javiervasgas@ingecsa.com" href="http://cordova.io">
        Ingecsa Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-targetSdkVersion" value="21" />
        <allow-intent href="market:*" />
    </platform>
    <plugin name="cordova-plugin-save-image" spec="~0.2.5" />
    <plugin name="cordova-plugin-photo-library" spec="~2.1.0">
        <variable name="PHOTO_LIBRARY_USAGE_DESCRIPTION" value="To choose photos" />
    </plugin>
	<preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
